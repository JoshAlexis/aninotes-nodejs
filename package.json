{
  "name": "aninotes-nodejs",
  "version": "1.0.0",
  "description": "API REST for management illustrators (personal likes) from pixiv, devianart and anime-pictures",
  "main": "index.js",
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "cross-env NODE_ENV=test jest --verbose --silent --runInBand",
    "test:watch": "npm run test -- --watch",
    "test:no-silent": "cross-env NODE_ENV=test jest --verbose",
    "test:open-handles": "cross-env NODE_ENV=test jest --verbose --silent --detectOpenHandles"
  },
  "keywords": [
    "API REST",
    "MongoDB"
  ],
  "author": "JoshAlexis",
  "license": "GPL",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-errors": "^1.8.0",
    "joi": "^17.4.0",
    "mongoose": "^5.12.2",
    "morgan": "^1.10.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.14",
    "cross-env": "^7.0.3",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "supertest": "^6.1.3"
  }
}
