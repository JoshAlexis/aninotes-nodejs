{
    "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true,
      "es6": true,
      "node": true,
      "jest": true
    },
    "extends": [
      "airbnb-base"
    ],
    "parserOptions": {
      "ecmaVersion": 12
    },
    "rules": {
      "class-methods-use-this": "off",
      "linebreak-style": ["error", "windows"],
      "consistent-return": "off",
      "no-underscore-dangle": "off",
      "no-param-reassign": "off",
      "no-unused-vars": "warn"
    }
}
