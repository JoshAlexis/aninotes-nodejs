{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "class-methods-use-this": "off",
        "linebreak-style": ["error", "windows"],
        "consistent-return": "off"
    }
}
